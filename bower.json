{
  "name": "example-app",
  "version": "0.1.0",
  "main": "public/javascripts/app.js",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "dependencies": {
    "swag": "~0.5.0",
    "lodash": "~2",
    "jquery": "~2",
    "backbone": "~1",
    "normalize-css": "~2",
    "h5bp-helpers": "~0.1",
    "font-awesome": "~4",
    "bluebird": "~2.9.3",
    "prelude-ls": "~1.1.1",
    "backbone-deep-model": "~0.10.4"
  },
  "overrides": {
    "backbone": {
      "main": "backbone.js",
      "dependencies": {
        "lodash": "*",
        "jquery": "*"
      }
    },
    "font-awesome": {
      "main": [
        "fonts",
        "css"
      ]
    },
    "swag": {
      "dependencies": {}
    },
    "prelude-ls": {
      "main": "browser/prelude-browser.js"
    },
    "backbone-deep-model": {
      "main": "distribution/deep-model.js",
      "dependencies": {
        "backbone": "*"
      }
    }
  }
